{
    "compilerOptions": {
        "moduleResolution": "node",
        "module": "commonjs",
        "target": "es5",
        "lib": [ "es2015", "dom" ],
        "outDir": "src",
        "allowSyntheticDefaultImports": true,
        "suppressImplicitAnyIndexErrors": true,
        "forceConsistentCasingInFileNames": true,
        "sourceMap": true,
        "declaration": true,
        "jsx": "react"
    },
    "files": [
        "./src/map.ts"
    ],
    "compileOnSave": false,
    "buildOnSave": false
}
