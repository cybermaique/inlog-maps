    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "ts-loader": "^5.3.0",
    "typescript": "^3.1.6"
  },
  "dependencies": {
    "awesome-typescript-loader": "^3.0.4-rc.2",
    "jsdoc-to-markdown": "^4.0.1",
    "leaflet": "^1.3.1",
    "leaflet-editable": "^1.1.0",
    "leaflet.path.drag": "0.0.6",
    "shx": "^0.2.2",
