{
  "name": "@inlog/inlog-maps",
  "version": "1.0.0",
  "description": "A library for using generic layer maps",
  "main": "lib/inlog-maps.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build && npm run test",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "mocha --require babel-core/register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-core/register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weareinlog/inlog-maps.git"
  },
  "keywords": [
    "maps",
    "map api"
  ],
  "author": "Carol Hoegen <carolhoegen@gmail.com>",
  "contributors": [
    "Lidia Freitas <lidiafreitas.me@gmail.com>",
    "Gustavo Santos <gustavo.dev@outlook.com.br>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/weareinlog/inlog-maps/issues"
  },
  "homepage": "https://github.com/weareinlog/inlog-maps",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "eslint": "^4.13.1",
    "eslint-loader": "^1.9.0",
    "mocha": "^4.0.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "leaflet": "^1.3.1",
    "leaflet-editable": "^1.1.0",
    "leaflet.path.drag": "0.0.6"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  }
}
