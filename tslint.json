{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "quotemark": [
            true,
            "single"
        ],
        "align": [
            true,
            "statements",
            "members"
        ],
        "trailing-comma": false,
        "no-console": [
            true,
            "warning"
        ],
    },
    "rulesDirectory": []
}
